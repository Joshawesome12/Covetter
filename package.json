{
  "name": "Covetter",
  "version": "1.0.0",
  "description": "A cover letter editing app",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=DEV concurrently \"babel-node server.js\" \"sleep 2 && electron .\"",
    "bundle": "webpack --mode=development",
    "watch": "NODE_ENV=DEV babel-node server.js",
    "start:prod": "webpack --mode=production && electron .",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "package": "webpack --mode=production && electron-packager .  \"Covetter\" --out=\"Packaged-App\" --icon=\"cvi.icns\" --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joshawesome12/Covetter.git"
  },
  "author": "Joshua Pawlik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Joshawesome12/Covetter/issues"
  },
  "homepage": "https://github.com/Joshawesome12/Covetter#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.8",
    "electron-packager": "^12.1.1",
    "electron-rebuild": "^1.8.2",
    "electron-reload": "^1.2.5",
    "express": "^4.16.3",
    "knex": "^0.15.2",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.23.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "sqlite3": "^4.0.2"
  }
}
